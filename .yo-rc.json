{
  "generator-jhipster": {
    "applicationType": "monolith",
    "gitHubOrganization": "whlee21",
    "baseName": "jhiptruffle",
    "packageName": "io.github.jhipster.truffle",
    "packageFolder": "io/github/jhipster/truffle",
    "serverPort": 8080,
    "serviceDiscoveryType": false,
    "authenticationType": "jwt",
    "uaaBaseName": "../uaa",
    "cacheProvider": "hazelcast",
    "enableHibernateCache": true,
    "websocket": false,
    "databaseType": "sql",
    "devDatabaseType": "h2Disk",
    "prodDatabaseType": "mariadb",
    "searchEngine": false,
    "enableSwaggerCodegen": "true",
    "messageBroker": false,
    "buildTool": "gradle",
    "useSass": true,
    "clientPackageManager": "yarn",
    "testFrameworks": [
      "gatling"
    ],
    "enableTranslation": true,
    "nativeLanguage": "ko",
    "languages": [
      "en",
      "ko"
    ],
    "clientFramework": "angularX",
    "jhiPrefix": "jhi"
  }
}